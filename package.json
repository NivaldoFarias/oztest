{
	"name": "oztest",
	"version": "0.0.1",
	"private": true,
	"description": "DevOZ - Tech test",
	"scripts": {
		"build": "bun run src/scripts/build.script.ts server",
		"build:seed": "bun run src/scripts/build.script.ts seed",
		"start": "node dist/server.js",
		"dev": "bun --watch src/scripts/server.script.ts",
		"dev:seed": "bun --watch src/scripts/seed.script.ts",
		"test": "bun test",
		"lint": "eslint .",
		"lint:fix": "eslint --fix .",
		"format": "prettier --write .",
		"server": "bun src/scripts/server.script.ts",
		"seed": "bun src/scripts/seed.script.ts",
		"docker:up": "docker compose -f docker/docker-compose.yml up -d mongodb",
		"docker:down": "docker compose -f docker/docker-compose.yml down"
	},
	"engines": {
		"node": ">=20.18.3"
	},
	"dependencies": {
		"@asteasolutions/zod-to-openapi": "^7.3.0",
		"@fastify/cors": "^10.0.2",
		"@fastify/sensible": "^6.0.3",
		"@fastify/static": "^8.1.1",
		"@fastify/swagger": "^9.4.2",
		"@googlemaps/google-maps-services-js": "^3.4.0",
		"@scalar/fastify-api-reference": "^1.25.122",
		"@typegoose/typegoose": "11.5.0",
		"fastify": "^5.2.1",
		"fastify-type-provider-zod": "^4.0.2",
		"mongoose": "7.5.3",
		"reflect-metadata": "0.1.13",
		"ts-pattern": "^5.6.2",
		"zod": "^3.24.2"
	},
	"devDependencies": {
		"@faker-js/faker": "^8.1.0",
		"@ianvs/prettier-plugin-sort-imports": "^4.4.1",
		"@total-typescript/ts-reset": "^0.6.1",
		"@types/bun": "^1.2.2",
		"@types/node": "^20.8.0",
		"@types/node-geocoder": "^4.2.6",
		"@typescript-eslint/eslint-plugin": "^8.24.1",
		"@typescript-eslint/parser": "^8.24.1",
		"eslint": "^9.20.1",
		"eslint-config-prettier": "^10.0.1",
		"eslint-plugin-prettier": "^5.2.3",
		"globals": "^15.15.0",
		"jiti": "^2.4.2",
		"prettier": "^3.5.1",
		"supertest": "^6.3.3",
		"typescript": "^5.7.3",
		"typescript-eslint": "^8.24.1"
	}
}
